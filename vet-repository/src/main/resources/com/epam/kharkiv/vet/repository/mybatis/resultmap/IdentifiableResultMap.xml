<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="identifiable">

    <resultMap type="com.epam.kharkiv.vet.domain.Identifiable" id="identifiableResultMap">
        <id property="id" column="id" />
    </resultMap>
    
	<sql id="identifiableInsertFields">
		id
	</sql>
	
	<sql id="identifiableInsertValues">
		#{id}
	</sql>
	
	<sql id="identifiableUpdateEquations">
		id = #{id}
	</sql>

</mapper>